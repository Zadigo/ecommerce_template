    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Python: Ecommerce Template",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Djano: Ecommerce Template",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\mywebsite\\manage.py",
            "args": [
                "runserver",
                // "--noreload"
            ],
            "django": true,
        }
    ]
}
